{
  "name": "tridactyl",
  "version": "0.1.0",
  "description": "Vimperator/Pentadactyl successor",
  "browser": {
    "url": false,
    "fs": false,
    "https": false,
    "http": false,
    "path": false,
    "timers": false,
    "stream": "stream-browserify"
  },
  "dependencies": {
    "@mozilla/readability": "^0.5.0",
    "D": "^1.0.0",
    "cleanslate": "^0.10.1",
    "compute-scroll-into-view": "^3.1.0",
    "csp-serdes": "github:cmcaine/csp-serdes",
    "css": "^3.0.0",
    "editor-adapter": "^0.0.5",
    "esbuild": "^0.24.0",
    "fuse.js": "^7.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "nearley": "^2.20.1",
    "ramda": "^0.30.1",
    "semver-compare": "^1.0.0",
    "stream-browserify": "^3.0.0",
    "tridactyl-arg": "git+https://github.com/GHolk/arg.git#v5.1.0",
    "ts-morph": "^24.0.0",
    "tsdef": "^0.0.14",
    "typedoc": "^0.26.11",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.2.3",
    "@types/css": "^0.0.38",
    "@types/firefox-webext-browser": "^120.0.4",
    "@types/jest": "^29.5.14",
    "@types/nearley": "^2.11.5",
    "@types/selenium-webdriver": "^4.1.27",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "command-line-args": "^5.2.1",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^50.4.3",
    "eslint-plugin-sonarjs": "^2.0.4",
    "geckodriver": "^5.0.0",
    "jest": "^29.7.0",
    "jest-webextension-mock": "^3.9.0",
    "marked": "^14.1.3",
    "prettier": "^3.3.3",
    "selenium-webdriver": "^4.26.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0",
    "web-ext": "^8.3.0"
  },
  "scripts": {
    "build": "./scripts/build.sh",
    "rebuild": "./scripts/build.sh --quick",
    "clean": "rm -rf build generated",
    "forrest-run": "bun run run",
    "jest": "jest --bail --runInBand",
    "lint": "bash hooks/pre-commit",
    "make-zip": "web-ext build --source-dir build --overwrite-dest",
    "pretty": "./scripts/pretty.sh",
    "run": "web-ext run -s build/ -u '0.0g.gg'",
    "test": "bun run build && bun run make-zip && jest --silent",
    "update-buildsystem": "rm -rf src/node_modules; bun clean",
    "watch": "echo 'watch is broken, use build instead'; exit 0;",
    "install": "git config core.hookspath hooks/"
  },
  "author": "Colin Caine",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/tridactyl/tridactyl.git"
  },
  "keywords": [
    "webextension",
    "webext",
    "vim",
    "firefox"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/tridactyl/tridactyl/issues"
  },
  "homepage": "https://github.com/tridactyl/tridactyl#readme"
}
